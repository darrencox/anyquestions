javascript:

  var count = 0

  window.setInterval(function(){
    
    if (count == 0){
      $('.hero').addClass('bg2');
      $('.hero').removeClass('bg1');
    } else if (count == 1){
      $('.hero').addClass('bg3');
      $('.hero').removeClass('bg2');
    } else {
      $('.hero').addClass('bg1');
      $('.hero').removeClass('bg3');
    };
    count++;
    if (count == 3) count = 0;
  },5000);